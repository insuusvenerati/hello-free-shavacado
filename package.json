{
  "name": "hello-free-shavacado",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "postinstall": "prisma generate",
    "build": "run-s build:*",
    "build:worker": "esbuild ./app/entry.worker.ts --outfile=./public/entry.worker.js --minify --bundle --format=esm --define:process.env.NODE_ENV='\"production\"'",
    "build:webworker": "esbuild ./app/utils/worker.ts --outfile=./public/worker.js --minify --bundle --format=esm --define:process.env.NODE_ENV='\"production\"'",
    "build:remix": "cross-env NODE_ENV=production remix build",
    "dev": "concurrently -c auto \"pnpm dev:webworker\" \"pnpm dev:remix\"",
    "dev:worker": "esbuild ./app/entry.worker.ts --outfile=./public/entry.worker.js --bundle --format=esm --define:process.env.NODE_ENV='\"development\"' --watch",
    "dev:webworker": "esbuild ./app/utils/worker.ts --outfile=./public/worker.js --bundle --format=esm --watch",
    "dev:remix": "remix dev",
    "format": "prettier --write .",
    "prisma:generate": "prisma generate",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve build",
    "typecheck": "tsc -b",
    "validate": "run-p lint typecheck",
    "clean": "rimraf build/ public/build",
    "semantic-release": "semantic-release"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build",
    "/recipe-data-scraper",
    "/patches",
    "/test-results",
    "/playwright-report",
    "/public/entry.worker.js"
  ],
  "dependencies": {
    "@mantine/hooks": "^6.0.13",
    "@prisma/client": "^4.15.0",
    "@remix-run/node": "1.17.1",
    "@remix-run/react": "1.17.1",
    "@remix-run/serve": "1.17.1",
    "@remix-run/server-runtime": "1.17.1",
    "@sentry/remix": "^7.55.2",
    "axios": "1.4.0",
    "bcryptjs": "^2.4.3",
    "cachified": "^3.5.4",
    "cheerio": "1.0.0-rc.12",
    "cloudinary": "^1.37.1",
    "clsx": "^1.2.1",
    "daisyui": "^3.1.1",
    "isbot": "^3.6.10",
    "iso8601-duration": "^2.1.1",
    "lru-cache": "^10.0.0",
    "lucide-react": "^0.245.0",
    "microdata-node": "^2.0.0",
    "nprogress": "^0.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-toastify": "^9.1.3",
    "recipe-data-scraper": "1.4.1",
    "remix": "1.17.1",
    "remix-flat-routes": "^0.5.8",
    "remix-typedjson": "^0.1.7",
    "remix-utils": "^6.4.1",
    "tailwind-merge": "^1.13.2",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@remix-run/dev": "1.17.1",
    "@remix-run/eslint-config": "1.17.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie": "^0.5.1",
    "@types/eslint": "^8.40.2",
    "@types/node": "^20.3.1",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "@types/semantic-release": "^20.0.1",
    "@types/workbox-core": "^5.0.0",
    "autoprefixer": "^10.4.14",
    "binode": "^1.0.5",
    "c8": "^8.0.0",
    "concurrently": "^8.2.0",
    "cookie": "^0.5.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.18.4",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "happy-dom": "^9.20.3",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "prettier": "2.8.8",
    "prisma": "^4.15.0",
    "rimraf": "^5.0.1",
    "semantic-release": "^21.0.5",
    "tailwindcss": "^3.3.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^3.12.7",
    "typescript": "5.1.3"
  },
  "engines": {
    "node": ">=18"
  },
  "prisma": {
    "seed": "npx tsx --experimental-fetch prisma/seed.ts"
  },
  "volta": {
    "node": "18.17.1",
    "pnpm": "8.3.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --cache --cache-location ./node_modules/.cache/eslint .",
      "prettier --write"
    ]
  },
  "pnpm": {
    "patchedDependencies": {
      "recipe-data-scraper@1.4.1": "patches/recipe-data-scraper@1.4.1.patch"
    }
  },
  "version": "0.0.0-development",
  "repository": {
    "type": "git",
    "url": "https://github.com/insuusvenerati/hello-free-shavacado.git"
  }
}
