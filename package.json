{
  "name": "remix-tailwind",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "postinstall": "prisma generate",
    "build": "run-s build:*",
    "build:css": "cross-env NODE_ENV=production postcss ./styles/tailwind.css -o app/styles/tailwind.css",
    "build:remix": "cross-env NODE_ENV=production remix build",
    "dev": "concurrently \"pnpm run dev:css\" \"pnpm run dev:remix\"",
    "dev:css": "postcss ./styles/tailwind.css -w -o app/styles/tailwind.css",
    "dev:remix": "cross-env NODE_ENV=development remix dev",
    "format": "prettier --write .",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:reset": "prisma migrate reset",
    "prisma:studio": "prisma studio",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "algolia:seed": "dotenv -e .env ts-node other/seedAlgolia.ts",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "start": "remix-serve build",
    "start:express": "node ./server.js",
    "start:mocks": "binode --require ./mocks -- @remix-run/serve:remix-serve build",
    "vitest": "vitest",
    "test:e2e:dev": "npx playwright test --trace=on",
    "test:e2e:run": "playwright test",
    "typecheck": "tsc -b",
    "validate": "run-p \"vitest --run\" lint typecheck test:e2e:run",
    "playwright:codegen": "playwright codegen localhost:3000 --viewport-size=1920,1080"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build",
    "/recipe-data-scraper",
    "/patches",
    "/test-results",
    "/playwright-report"
  ],
  "dependencies": {
    "@mantine/hooks": "^5.10.3",
    "@prisma/client": "^4.9.0",
    "@remix-run/express": "^1.12.0",
    "@remix-run/node": "^1.12.0",
    "@remix-run/react": "^1.12.0",
    "@remix-run/serve": "^1.12.0",
    "@remix-run/server-runtime": "^1.12.0",
    "axios": "1.2.6",
    "bcryptjs": "^2.4.3",
    "cheerio": "1.0.0-rc.12",
    "clsx": "^1.2.1",
    "compression": "^1.7.4",
    "daisyui": "^2.50.0",
    "express": "^4.18.2",
    "isbot": "^3.6.5",
    "iso8601-duration": "^2.1.1",
    "microdata-node": "^2.0.0",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recipe-data-scraper": "1.4.1",
    "remix": "1.12.0",
    "remix-typedjson": "^0.1.7",
    "remix-utils": "^6.0.0",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@playwright/test": "^1.30.0",
    "@remix-run/dev": "^1.12.0",
    "@remix-run/eslint-config": "^1.12.0",
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/dom": "^8.20.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie": "^0.5.1",
    "@types/eslint": "^8.4.10",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/workbox-core": "^5.0.0",
    "@vitejs/plugin-react": "^3.0.1",
    "@vitest/coverage-c8": "^0.28.3",
    "autoprefixer": "^10.4.13",
    "binode": "^1.0.5",
    "c8": "^7.12.0",
    "concurrently": "^7.6.0",
    "cookie": "^0.5.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-playwright": "^0.12.0",
    "happy-dom": "^8.2.2",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "msw": "^1.0.0",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "patch-package": "^6.5.1",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "postcss-lightningcss": "^0.6.0",
    "prettier": "2.8.3",
    "prettier-plugin-tailwindcss": "^0.2.2",
    "prisma": "^4.9.0",
    "start-server-and-test": "^1.15.3",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "tsx": "^3.12.2",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vite-tsconfig-paths": "^4.0.5",
    "vitest": "^0.28.4"
  },
  "engines": {
    "node": ">=16"
  },
  "prisma": {
    "seed": "npx tsx --experimental-fetch prisma/seed.ts"
  },
  "volta": {
    "node": "18.13.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --cache --cache-location ./node_modules/.cache/eslint .",
      "prettier --write"
    ]
  },
  "pnpm": {
    "patchedDependencies": {
      "recipe-data-scraper@1.4.1": "patches/recipe-data-scraper@1.4.1.patch"
    }
  }
}
