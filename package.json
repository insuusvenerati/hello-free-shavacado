{
  "name": "hello-free-shavacado",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "postinstall": "prisma generate",
    "build": "run-s build:*",
    "build:worker": "esbuild ./app/entry.worker.ts --outfile=./public/entry.worker.js --minify --bundle --format=esm --define:process.env.NODE_ENV='\"production\"'",
    "build:webworker": "esbuild ./app/utils/worker.ts --outfile=./public/worker.js --minify --bundle --format=esm --define:process.env.NODE_ENV='\"production\"'",
    "build:remix": "cross-env NODE_ENV=production remix build",
    "dev": "pnpm build:remix && concurrently -c auto \"pnpm dev:webworker\" \"pnpm dev:remix\"",
    "dev:worker": "esbuild ./app/entry.worker.ts --outfile=./public/entry.worker.js --bundle --format=esm --define:process.env.NODE_ENV='\"development\"' --watch",
    "dev:webworker": "esbuild ./app/utils/worker.ts --outfile=./public/worker.js --bundle --format=esm --watch",
    "dev:remix": "remix dev",
    "dev:server": "cross-env NODE_ENV=development tsx watch --clear-screen=false ./server.ts",
    "format": "prettier --write .",
    "prisma:generate": "prisma generate",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve build",
    "start:express": "node ./server.js",
    "typecheck": "tsc -b",
    "validate": "run-p lint typecheck",
    "clean": "rimraf build/ public/build",
    "semantic-release": "semantic-release"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build",
    "/recipe-data-scraper",
    "/patches",
    "/test-results",
    "/playwright-report",
    "/public/entry.worker.js"
  ],
  "dependencies": {
    "@mantine/hooks": "^6.0.8",
    "@next-boost/hybrid-disk-cache": "^0.3.0",
    "@prisma/client": "^4.13.0",
    "@remix-run/express": "1.15.0",
    "@remix-run/node": "1.15.0",
    "@remix-run/react": "1.15.0",
    "@remix-run/serve": "1.15.0",
    "@remix-run/server-runtime": "1.15.0",
    "@sentry/remix": "^7.48.0",
    "axios": "1.3.5",
    "bcryptjs": "^2.4.3",
    "cheerio": "1.0.0-rc.12",
    "cloudinary": "^1.36.1",
    "clsx": "^1.2.1",
    "compression": "^1.7.4",
    "daisyui": "^2.51.5",
    "express": "^4.18.2",
    "isbot": "^3.6.8",
    "iso8601-duration": "^2.1.1",
    "lucide-react": "^0.162.0",
    "microdata-node": "^2.0.0",
    "morgan": "^1.10.0",
    "nprogress": "^0.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-toastify": "^9.1.2",
    "recipe-data-scraper": "1.4.1",
    "remix": "1.15.0",
    "remix-image": "^1.4.0",
    "remix-image-sharp": "^0.1.4",
    "remix-typedjson": "^0.1.7",
    "remix-utils": "^6.1.0",
    "sharp": "^0.32.0",
    "tailwind-merge": "^1.12.0",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@remix-run/dev": "1.15.0",
    "@remix-run/eslint-config": "1.15.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cookie": "^0.5.1",
    "@types/eslint": "^8.37.0",
    "@types/node": "^18.14.4",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@types/semantic-release": "^20.0.1",
    "@types/workbox-core": "^5.0.0",
    "autoprefixer": "^10.4.14",
    "binode": "^1.0.5",
    "c8": "^7.13.0",
    "concurrently": "^8.0.1",
    "cookie": "^0.5.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.17.17",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "happy-dom": "^8.9.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "prettier": "2.8.7",
    "prisma": "^4.13.0",
    "rimraf": "^4.1.3",
    "semantic-release": "^20.1.1",
    "tailwindcss": "^3.3.1",
    "tsconfig-paths": "^3.14.2",
    "tsx": "^3.12.6",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=18"
  },
  "prisma": {
    "seed": "npx tsx --experimental-fetch prisma/seed.ts"
  },
  "volta": {
    "node": "18.16.0",
    "pnpm": "8.3.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --cache --cache-location ./node_modules/.cache/eslint .",
      "prettier --write"
    ]
  },
  "pnpm": {
    "patchedDependencies": {
      "recipe-data-scraper@1.4.1": "patches/recipe-data-scraper@1.4.1.patch"
    }
  },
  "version": "0.0.0-development",
  "repository": {
    "type": "git",
    "url": "https://github.com/insuusvenerati/hello-free-shavacado.git"
  }
}
