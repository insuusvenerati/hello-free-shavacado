{
  "name": "hello-free-shavacado",
  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "repository": "git@github.com:insuusvenerati/hello-free-shavacado.git",
  "author": "Sean Norwood <norwood.sean@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "test": "yarn workspace @stiforr/frontend test --watch",
    "lint": "turbo run lint",
    "stop": "turbo run stop",
    "start": "turbo run start",
    "nps:prepare": "nps prepare",
    "prisma:migrate": "yarn workspace @stiforr/backend prisma migrate",
    "prisma:studio": "yarn workspace @stiforr/backend prisma studio",
    "prisma:generate": "yarn workspace @stiforr/backend prisma generate",
    "e2e:open": "cross-env NODE_ENV=test turbo run e2e:open",
    "e2e:run": "cross-env NODE_ENV=test turbo run e2e:run",
    "fix-mismatches": "pnpm syncpack fix-mismatches"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "devDependencies": {
    "cross-env": "^7.0.3",
    "dotenv": "16.0.3",
    "eslint": "^8.25.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "nps": "5.10.0",
    "nps-utils": "1.7.0",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "turbo": "1.5.6",
    "syncpack": "^8.2.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint",
      "prettier --write"
    ]
  },
  "volta": {
    "node": "16.18.0",
    "yarn": "1.22.19",
    "npm": "8.19.2"
  },
  "engines": {
    "node": "16.x"
  },
  "syncpack": {
    "workspace": false
  }
}
