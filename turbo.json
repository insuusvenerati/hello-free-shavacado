{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "dev": {
      "cache": false
    },
    "build": {
      "dependsOn": [
        "^build"
      ]
    },
    "@stiforr/frontend#build": {
      "dependsOn": [
        "$NEXT_PUBLIC_CLERK_FRONTEND_API",
        "$NEXT_PUBLIC_API_URL"
      ]
    },
    "@stiforr/backend#build": {
      "dependsOn": [
        "$DATABASE_URL"
      ]
    },
    "test": {
      "dependsOn": [
        "^build"
      ],
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "stop": {
      "cache": false
    },
    "start": {
      "cache": false
    },
    "e2e:open": {
      "cache": false
    },
    "e2e:run": {
      "dependsOn": ["build"]
    }
  }
}
