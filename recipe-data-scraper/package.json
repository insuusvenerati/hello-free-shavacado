{
  "name": "recipe-data-scraper",
  "version": "1.4.1",
  "description": "web scraper for recipe data",
  "repository": "github:thelifenadine/recipe-data-scraper",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "webpack",
    "build-watch": "webpack -w",
    "test-old": "mocha './src/**/*.test.js' --require @babel/register --reporter spec || exit 0",
    "lint": "eslint './src/**/*.js'",
    "test": "cross-env NODE_ENV=test nyc mocha './src/**/*.test.js' --exit",
    "coveralls": "cat ./coverage/lcov.info | node ./node_modules/coveralls/bin/coveralls.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls && nyc report --reporter=lcov",
    "print-recipe-tester": "cross-env LOGGING_ENABLED=true babel-node ./scripts/printRecipeTester",
    "test-url": "cross-env LOGGING_ENABLED=true babel-node ./scripts/urlTesterCLI"
  },
  "author": "Nadine Kinnen",
  "license": "Apache-2.0",
  "dependencies": {
    "axios": "0.25.0",
    "cheerio": "1.0.0-rc.10",
    "iso8601-duration": "1.3.0",
    "microdata-node": "2.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.20.2",
    "@babel/node": "7.20.2",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-transform-runtime": "7.19.6",
    "@babel/preset-env": "7.20.2",
    "@babel/register": "7.18.9",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.3.0",
    "babel-plugin-istanbul": "6.1.1",
    "chai": "4.3.4",
    "clean-webpack-plugin": "3.0.0",
    "coveralls": "3.1.1",
    "cross-env": "7.0.3",
    "eslint": "7.3.1",
    "eslint-loader": "4.0.2",
    "eslint-plugin-babel": "5.3.1",
    "istanbul": "0.4.5",
    "mocha": "9.1.4",
    "mocha-lcov-reporter": "1.3.0",
    "nyc": "15.1.0",
    "proxyquire": "2.1.3",
    "readline-sync": "1.4.10",
    "sinon": "12.0.1",
    "webpack": "4.43.0",
    "webpack-cli": "4.9.1"
  },
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
